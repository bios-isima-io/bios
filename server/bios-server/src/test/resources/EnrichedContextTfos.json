{
  "name": "enrichedContext",
  "type": "context",
  "missingValuePolicy": "strict",
  "version": 1588175368660,
  "attributes": [
    {
      "name": "resourceId",
      "type": "string"
    },
    {
      "name": "resourceTypeNum",
      "type": "long"
    },
    {
      "name": "zipcode",
      "type": "string",
      "defaultValue": "missing_zipcode",
      "missingValuePolicy": "use_default"
    }
  ],
  "globalLookupPolicy": "fail_parent_lookup",
  "enrichments": [
    {
      "enrichmentName": "resourceType",
      "foreignKey": [
        "resourceTypeNum"
      ],
      "enrichedAttributes": [
        {
          "value": "resourceType.resourceTypeCode"
        },
        {
          "value": "resourceType.resourceCategory",
          "as": "myResourceCategory"
        }
      ]
    },
    {
      "enrichmentName": "resourceDetails",
      "foreignKey": [
        "resourceId"
      ],
      "enrichedAttributes": [
        {
          "valuePickFirst": [
            "staff.cost",
            "doctor.salary",
            "venue.cost",
            "auxiliary.cost",
            "machine.cost",
            "workspace.rent"
          ],
          "as": "cost"
        },
        {
          "valuePickFirst": [
            "staff.aliasName",
            "doctor.aliasName",
            "venue.venueId",
            "auxiliary.auxiliaryId",
            "machine.machineId",
            "workspace.workspaceId"
          ],
          "as": "resourceValue"
        }
      ]
    }
  ],
  "auditEnabled": false,
  "ttl": 86400000
}
