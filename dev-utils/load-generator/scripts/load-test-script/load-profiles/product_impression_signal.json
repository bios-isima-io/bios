  {
    "signalName": "productImpressionSignal",
    "peakRequestSize": 6250,
    "missingAttributePolicy": "Reject",
    "attributes": [
      {
        "attributeName": "visitorId",
        "type": "string",
        "attrValType": "STRING_ID",
        "range": "5-5"
      },
      {
        "attributeName": "userId",
        "type": "integer",
        "attrValType": "NUMERIC_ID",
        "range": "10000-99999"
      },
      {
        "attributeName": "funnel",
        "type": "string",
        "attrValType": "ENUM",
	      "allowedValues": [
          "email",
          "blogpost",
          "google ad",
          "facebook ad",
          "twitter ad"
        ]
      },
      {
        "attributeName": "section",
        "type": "string",
        "attrValType": "DEPARTMENT",
        "range": "5-15"
      },
      {
        "attributeName": "subsection",
        "type": "string",
        "attrValType": "DEPARTMENT",
        "range": "5-15"
      },
      {
        "attributeName": "funnelTile",
        "type": "string",
        "attrValType": "ENUM",
	      "allowedValues": [
          "Kitchen",
          "Shoes",
          "Apparel",
          "Dairy",
          "ReadyToEat",
          "Meat",
          "Fruits",
          "Vegetables"
        ]
      },
      {
        "attributeName": "funnelRow",
        "type": "integer",
        "attrValType": "RANDOM_NUMBER",
        "range": "1-49"
      },
      {
        "attributeName": "sortbar",
        "type": "string",
        "attrValType": "DEPARTMENT",
        "range": "5-15"
      },
      {
        "attributeName": "sortbarGroup",
        "type": "string",
        "attrValType": "DEPARTMENT",
        "range": "5-15"
      },
      {
        "attributeName": "funnelSection",
        "type": "string",
        "attrValType": "DEPARTMENT",
        "range": "5-15"
      },
      {
        "attributeName": "receivedAt",
        "type": "integer",
        "attrValType": "TIMESTAMP"
      },
      {
        "attributeName": "productId",
        "type": "string",
        "attrValType": "REGEX",
        "pattern": "[1-5]{5}"
      },
      {
        "attributeName": "plp",
        "type": "string",
        "attrValType": "RANDOM_STRING",
        "range": "5-5"
      }
    ]
  }
