{
  "signalName": "patientVisits",
  "peakRequestSize": 3125,
  "missingAttributePolicy": "Reject",
  "attributes": [
    {"attributeName": "visitId", "type": "integer",
      "attrValType": "NUMERIC_ID", "range": "100000-999999" },
    {"attributeName": "patientId", "type": "integer",
      "attrValType": "NUMERIC_ID", "range": "10000-99999" },
    {"attributeName": "resourceId", "type": "string",
      "attrValType": "REGEX", "pattern": "[r]{1}[e]{1}[s]{1}[1-6]{1}[a-z]{1}" }
  ],
  "enrich": {
    "enrichments": [
      {
        "enrichmentName": "resourceDetails",
        "foreignKeys": ["resourceId"],
        "missingLookupPolicy": "Reject",
        "contextName": "enrichedContext",
        "contextAttributes": [
          {"attributeName": "resourceTypeNum"},
          {"attributeName": "zipcode"},
          {"attributeName": "resourceTypeCode"},
          {"attributeName": "myResourceCategory"},
          {"attributeName": "cost"},
          {"attributeName": "resourceValue"}
        ]
      }
    ]
  }
}
