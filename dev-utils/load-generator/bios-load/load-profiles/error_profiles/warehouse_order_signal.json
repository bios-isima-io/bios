      "attrValType": "NUMERIC_ID",
      "range": "10000-99999"
    },
    {
      "attributeName": "pickerTaskId",
      "type": "integer",
      "attrValType": "NUMERIC_ID",
      "range": "100000-999999"
    },
    {
