      "attrValType": "NAME"
    },
    {
      "attributeName": "createdAt",
      "type": "integer",
      "attrValType": "TIMESTAMP"
    },
    {
      "attributeName": "updatedBy",
      "type": "string",
      "attrValType": "NAME"
    },
    {
      "attributeName": "updatedAt",
      "type": "integer",
      "attrValType": "TIMESTAMP"
    },
    {
      "attributeName": "orderEDD",
      "type": "integer",
      "attrValType": "TIMESTAMP"
    },
    {
      "attributeName": "warehouseTDD",
      "type": "integer",
      "attrValType": "TIMESTAMP"
    },
    {
      "attributeName": "totalAmount",
      "type": "decimal",
      "attrValType": "PRICE",
      "range": "100-99999"
    },
    {
      "attributeName": "payableAmount",
      "type": "decimal",
      "attrValType": "PRICE",
