      "NORMAL",
      "HIGH",
      "EXPRESS"
    ]
  },
  {
    "attributeName": "status",
    "type": "string",
    "attrValType": "ENUM",
    "allowedValues": [
      "READY_FOR_BILLING",
      "PENDING_FOR_STOCK",
      "CANCELLED",
      "PICKING_IN_PROGRESS",
      "RECEIVED_AT_STORE",
      "INVENTORY_BLOCK_PENDING",
